package pages

import "dwelr/views/layouts"
import "dwelr/views/components"

templ ResultPage() {
	@layouts.Base() {
		<div class="py-8 px-52 flex flex-col items-center">
			/* Search bar component */
			@components.SearchBar()
			<h2 class="font-bold text-xl mt-10 mb-5 mr-auto">Search Results:</h2>
			@components.ListingCard()
		</div>
	}
}
